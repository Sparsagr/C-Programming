//(4) c PROGRAM (IF-ELSE):: leap year...!!
#include <stdio.h>
int main() 
{
    int a;
    printf("enter two numbers::");
    scanf("%d",&a);
    if(a % 400 ==0)
    {
        printf("a is a leap year");
    }
    else if(a % 100 ==0 )
    {
        printf("a is not a leap year");
    }
    else if(a % 4 == 0)
    {
        printf("a is a leap year");
    }
    else
    {
        printf("a is not a leap year");
    }
    return 0;
}
