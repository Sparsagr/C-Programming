// (1)c PROGRAM (IF-ELSE):: MAXIMUM OF TWO NUMBERS ...!!
#include <stdio.h>
int main() 
{
    int a,b;
    printf("enter two numbers::");
    scanf("%d%d",&a,&b);
    if(a>b)
    {
        printf("a is greater",a);
    }
    if(b>a)
    {
        printf("b is greater",b);
    }
    if(a==b)
    {
        printf("both are euqal");
    }
    return 0;
}
