//(1) c PROGRAM (SWITCH CASE):: MAXIMIM AND MINIMUM BETWEEN TWO NUMBERS...!!
#include <stdio.h>
int main() 
{
    int a,b;
    printf("enter a number between:(1-7):");
    scanf("%d%d",&a,&b);
    switch(a)
    {
        case 1:
        printf("MONDAY");
        break;
        case 2:
        printf("TUESDAY");
        break;
        case 3:
        printf("WEDNESDAY");
        break;
        case 4:
        printf("THURSDAY");
        break;
        case 5:
        printf("FRIDAY");
        break;
        case 6:
        printf("SATURDAY");
        break;
        case 7:
        printf("SUNDAY");
        break;
    }
    return 0;
}
