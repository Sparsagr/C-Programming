// (3)c PROGRAM (IF-ELSE):: maximum and minimum...!!
#include <stdio.h>
int main() 
{
    int a,b;
    printf("enter a number:");
    scanf("%d%d",&a,&b);
    switch(a>b)
    {
        case 0:
        printf("%d is maximum",a);
        break;
        case 1:
        printf("%d  is minimum",b);
        break;
    }
    return 0;
}
